{
  "expo": {
    "name": "Expo Mega Demo",
    "slug": "expo-mega-demo",
    "version": "0.1.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "demo",
    "platforms": [
      "android",
      "ios"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "jsEngine": "hermes",
    "ios": {
      "bundleIdentifier": "com.barthap.expomegademo",
      "supportsTablet": false,
      "infoPlist": {
        "NSAppleMusicUsageDescription": "Allow $(PRODUCT_NAME) to access your music library"
      }
    },
    "android": {
      "jsEngine": "jsc",
      "package": "com.barthap.expomegademo",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "BLUETOOTH_ADVERTISE",
        "BLUETOOTH_SCAN",
        "BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN"
      ]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      "@config-plugins/react-native-ble-plx",
      "./patches/fix-entitlements.plugin",
      "./custom_native_modules/expo-music-picker/app.plugin"
    ],
    "extra": {
      "eas": {
        "projectId": "ea2ef501-9070-4ea5-bd46-eedbfcf4f1aa"
      }
    }
  }
}
